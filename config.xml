<?xml version="1.0" encoding="utf-8"?>
<control  xmlns="http://www.askia.com/2.2.0/ADCSchema"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://www.askia.com/2.2.0/ADCSchema https://raw.githubusercontent.com/AskiaADX/ADXSchema/2.2.0/ADCSchema.xsd"
          version="2.2.0"
          askiaCompat="5.5.2">
  <info>
    <name>Slider</name>
    <guid>75419bde-c158-45fe-a9b7-68cd4c6bb9b0</guid>
    <version>4.1.5</version>
    <date>2021-02-02</date>
    <description><![CDATA[]]></description>
    <company>Askia</company>
    <author><![CDATA[Paul Ferreira <paulf@askia.com>,	  Thibault Piriou <thibault@askia.com>	  Jérôme Duparc <jeromed@askia.com>	  Stephen Bronnec <stephen@askia.com>   Prabath Gamage <prabath@askia.com>]]></author>
    <site>http://www.askia.com</site>
    <helpURL>https://dev.askia.com</helpURL>
    <categories>
      <category>General</category>
    </categories>
    <constraints>
      <constraint on="questions" numeric="true" single="true" chapter="false" date="false" multiple="false" open="false" requireParentLoop="false" requireLoopDepth="0" manageHeader="true"/>
      <constraint on="controls" responseblock="true" textbox="true" checkbox="false" label="false" listbox="false" radiobutton="false" />
      <constraint on="responses" max="*" min="*" />
    </constraints>
  </info>
  <outputs defaultOutput="fallback">
    <output id="fallback" manageLoopDepth="1">
      <description><![CDATA[Fallback when the browser doesnt support Javascript]]></description>
      <condition><![CDATA[Not(Browser.Support("javascript"))]]></condition>
      <content fileName="default.css" type="css" mode="static" position="head" />
      <content fileName="numeric.html" type="html" mode="dynamic" position="none" />
      <content fileName="single.html" type="html" mode="dynamic" position="none" />
      <content fileName="numeric_loop.html" type="html" mode="dynamic" position="none" />
      <content fileName="single_loop.html" type="html" mode="dynamic" position="none" />
      <content fileName="default.html" type="html" mode="dynamic" position="placeholder" />
    </output>
    <output id="standard"  manageLoopDepth="1">
      <description><![CDATA[
        Output when the browser support Javascript
      ]]></description>
      <condition><![CDATA[
        Browser.Support("javascript")
      ]]></condition>
      <content fileName="slider.css" type="css" mode="static" position="head" />
      <content fileName="standard.css" type="css" mode="dynamic" position="head" />
      <content fileName="jquery.nouislider.min.css" type="css" mode="share" position="head" />
      <content fileName="jquery.nouislider.pips.min.css" type="css" mode="share" position="head" />
      <content fileName="tippy.css" type="css" mode="share" position="head" />
      <content fileName="standard.html" type="html" mode="dynamic" position="placeholder" />
      <content fileName="standard_numeric.html" type="html" mode="dynamic" position="none" />
      <content fileName="standard_single.html" type="html" mode="dynamic" position="none" />
      <content fileName="standard_numeric_loop.html" type="html" mode="dynamic" position="none" />
      <content fileName="standard_single_loop.html" type="html" mode="dynamic" position="none" />
      <content fileName="es5-shim.min.js" type="javascript" mode="share" position="head">
        <yield><![CDATA[
          <!--[if lt IE 10]>
            <script type="text/javascript" src="{%= CurrentADC.URLTo("share/es5-shim.min.js") %}"></script>
            <![endif]-->
        ]]></yield>
      </content>
      <content fileName="jquery-1.11.2.min.js" type="javascript" mode="share" position="head" />
      <content fileName="jquery.nouislider.min.js" type="javascript" mode="share" position="head" />
      <content fileName="jquery.nouislider.all.min.js" type="javascript" mode="share" position="head" />
      <content fileName="slider.js" type="javascript" mode="static" position="head" />
      <content fileName="standard.js" type="javascript" mode="dynamic" position="foot" />
      <content fileName="standard_numeric.js" type="javascript" mode="dynamic" position="none" />
      <content fileName="standard_single.js" type="javascript" mode="dynamic" position="none" />
      <content fileName="standard_numeric_loop.js" type="javascript" mode="dynamic" position="none" />
      <content fileName="standard_single_loop.js" type="javascript" mode="dynamic" position="none" />
      <content fileName="interconnection.js" type="javascript" mode="dynamic" position="foot" />
      <content fileName="tippy.min.js" type="javascript" mode="share" position="foot" />
    </output>
  </outputs>
  <properties>
    <category id="numericQuestion" name="Numeric question">
      <property xsi:type="standardProperty" id="unitStep" name="Step for numeric" type="number" mode="static" require="false" visible="true">
        <description><![CDATA[Using HTML 5, in modern browser.
          Indicates the unit to increment or decrement the number using the stepper.]]></description>
        <value><![CDATA[1]]></value>
      </property>
      <property xsi:type="standardProperty" id="decimalPlaces" name="Decimal Places" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[Set the number of decimal places]]></description>
        <value><![CDATA[0]]></value>
        <options>
          <option value="0" text="0" />
          <option value="1" text="1" />
          <option value="2" text="2" />
        </options>
      </property>
    </category>
    <category id="sliderSettings" name="Slider Settings">
      <property xsi:type="standardProperty" id="sliderOrientation" name="Slider Orientation" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The orientation of the slider]]></description>
        <value><![CDATA[horizontal]]></value>
        <options>
          <option value="horizontal" text="Horizontal" />
          <option value="vertical" text="Vertical" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="sliderDirection" name="Slider Direction" type="string" mode="dynamic" require="false" visible="true">
        <description><![CDATA[The direction of the slider]]></description>
        <value><![CDATA[ltr]]></value>
        <options>
          <option value="ltr" text="Left to right" />
          <option value="rtl" text="Right to left" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="minValue" name="Minimum value" type="number" mode="static" require="false" visible="true">
        <description><![CDATA[Set min slider value]]></description>
        <value><![CDATA[0]]></value>
      </property>
      <property xsi:type="standardProperty" id="intermediateValue" name="Intermediate value" type="number" mode="static" require="false" visible="true">
        <description><![CDATA[Set intermediate slider value (the value at 50% of the slider)]]></description>
        <value></value>
      </property>
      <property xsi:type="standardProperty" id="maxValue" name="Maximum value" type="number" mode="static" require="false" visible="true">
        <description><![CDATA[Set max slider value]]></description>
        <value><![CDATA[10]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderHeight" name="Slider Height (vertical layout)" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The slider height]]></description>
        <value><![CDATA[200px]]></value>
      </property>
      <property xsi:type="standardProperty" id="showTooltips" name="Show tooltips" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Show tooltips when moving slider.]]></description>
        <value><![CDATA[0]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="showMarkers" name="Show markers" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Show markers below base.]]></description>
        <value><![CDATA[1]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="showSubMarkers" name="Show sub markers" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Show sub markers]]></description>
        <value><![CDATA[0]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="showMarkerText" name="Show marker text" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Show marker text below markers.]]></description>
        <value><![CDATA[1]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="stepMarkerText" name="Step value for markers" type="number" mode="static" require="false" visible="true">
        <description><![CDATA[Using HTML 5, in modern browser.
          Indicates the unit to seperate the numeric value/marker text of markers.]]></description>
        <value><![CDATA[1]]></value>
      </property>
      <property xsi:type="standardProperty" id="showResponseCaptions" name="Show response captions" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Show response captions for single-coded questions.]]></description>
        <value><![CDATA[0]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
    </category>
    <category id="sliderBaseStyling" name="Slider Base Styling">
      <property xsi:type="standardProperty" id="sliderBaseHeight" name="Slider Base Height" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The height or width of the handle depending on orientation]]></description>
        <value><![CDATA[24]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderBasePrimaryColour" name="Slider Base Primary Colour" type="color" mode="dynamic" require="true" visible="false">
        <description><![CDATA[Primary colour of the slider]]></description>
        <value><![CDATA[255,0,0]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderBaseSecondaryColour" name="Slider Base Secondary Colour" type="color" mode="dynamic" require="true" visible="false">
        <description><![CDATA[Secondary colour of the slider]]></description>
        <value><![CDATA[0,255,0]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderBaseGradientDirection" name="Slider Base Gradient Direction" type="string" mode="dynamic" require="false" visible="true">
        <description><![CDATA[The direction of the gradient on the base]]></description>
        <value><![CDATA[ltr]]></value>
        <options>
          <option value="ltr" text="Left to Right" />
          <option value="ttb" text="Top to Bottom" />
          <option value="rtl" text="Right to Left" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="sliderBaseBorderRadius" name="Slider Base Border Radius" type="string" mode="dynamic" require="true" visible="false">
        <description><![CDATA[The amount of curve on the slider base corners]]></description>
        <value><![CDATA[{%= Theme.BorderRadius %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderBaseBorderColour" name="Slider Base Border Colour" type="color" mode="dynamic" require="true" visible="false">
        <description><![CDATA[Border colour of slider base]]></description>
        <value><![CDATA[{%= Theme.NeutralDarkColor %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderBaseBorderThickness" name="Slider Base Border Thickness" type="string" mode="dynamic" require="true" visible="false">
        <description><![CDATA[The thickness of the border around the slider base]]></description>
        <value><![CDATA[{%= Theme.BorderWidth %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderBaseBorderStyle" name="Slider Base Border Style" type="string" mode="static" require="false" visible="false">
        <description><![CDATA[The style of the border around the slider base]]></description>
        <value><![CDATA[solid]]></value>
        <options>
          <option value="none" text="None" />
          <option value="solid" text="Solid" />
          <option value="dashed" text="Dashed" />
          <option value="dotted" text="Dotted" />
          <option value="double" text="Double" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="sliderSelectedPrimaryColour" name="Slider Selected Primary Colour" type="color" mode="dynamic" require="false" visible="true">
        <description><![CDATA[Primary colour of the slider once a value has been set]]></description>
        <value><![CDATA[255,0,0]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderSelectedSecondaryColour" name="Slider Selected Secondary Colour" type="color" mode="dynamic" require="false" visible="true">
        <description><![CDATA[Secondary colour of the slider once a value has been set]]></description>
        <value><![CDATA[0,255,0]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderMarkerColor" name="Slider Marker Color" type="color" mode="dynamic" require="true" visible="false">
        <description><![CDATA[Marker colour of slider]]></description>
        <value><![CDATA[{%= Theme.NeutralDarkColor %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderConnect" name="Slider Connect" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Connect the slider]]></description>
        <value><![CDATA[false]]></value>
        <options>
          <option value="lower" text="Yes" />
          <option value="false" text="No" />
        </options>
      </property>
    </category>
    <category id="sliderHandleStyling" name="Slider Handle Styling">
      <property xsi:type="standardProperty" id="sliderHandleHeight" name="Slider Handle Height" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The height of the handle]]></description>
        <value><![CDATA[34]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderHandleWidth" name="Slider Handle Width" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The width of the handle]]></description>
        <value><![CDATA[44]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderHandleStartPosition" name="Slider Handle Start Position" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The start position of the handle]]></description>
        <value><![CDATA[half]]></value>
        <options>
          <option value="min" text="Min" />
          <option value="half" text="Half" />
          <option value="max" text="Max" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="hideHandle" name="Hide handle at start" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Show or hide handle if no value is set]]></description>
        <value><![CDATA[1]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="sliderHandlePrimaryColour" name="Slider Handle Background Colour" type="color" mode="dynamic" require="true" visible="false">
        <description><![CDATA[Background colour of the Handle]]></description>
        <value><![CDATA[{%= Theme.NeutralLightColor %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderHandleBorderColour" name="Slider Handle Border Colour" type="color" mode="dynamic" require="true" visible="false">
        <description><![CDATA[Border colour of slider Handle]]></description>
        <value><![CDATA[{%= Theme.NeutralDarkColor %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderHandleBorderRadius" name="Slider Handle Border Radius" type="string" mode="dynamic" require="true" visible="false">
        <description><![CDATA[The amount of curve on the slider handle corners]]></description>
        <value><![CDATA[{%= Theme.BorderRadius %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderHandleBorderThickness" name="Slider Handle Border Thickness" type="string" mode="static" require="true" visible="false">
        <description><![CDATA[The thickness of the border around the slider Handle]]></description>
        <value><![CDATA[{%= Theme.BorderWidth %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderHandleBorderStyle" name="Slider Handle Border Style" type="string" mode="static" require="false" visible="false">
        <description><![CDATA[The style of the border around the slider Handle]]></description>
        <value><![CDATA[solid]]></value>
        <options>
          <option value="none" text="None" />
          <option value="solid" text="Solid" />
          <option value="dashed" text="Dashed" />
          <option value="dotted" text="Dotted" />
          <option value="double" text="Double" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="sliderHandleDropShadow" name="Slider Handle Drop Shadow" type="string" mode="static" require="false" visible="false">
        <description><![CDATA[The visibility of a dropshadow under the handle]]></description>
        <value><![CDATA[none]]></value>
        <options>
          <option value="0px 2px 4px rgba(0,0,0,.2)" text="On" />
          <option value="none" text="Off" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="sliderFocusPrimaryColour" name="Slider Focus Background Colour" type="color" mode="dynamic" require="true" visible="false">
        <description><![CDATA[Background colour of the Focus]]></description>
        <value><![CDATA[{%= Theme.NeutralLightColor %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderFocusBorderColour" name="Slider Focus Border Colour" type="color" mode="dynamic" require="true" visible="false">
        <description><![CDATA[Border colour of slider Focus]]></description>
        <value><![CDATA[{%= Theme.NeutralDarkColor %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="sliderFocusBorderThickness" name="Slider Focus Border Thickness" type="string" mode="dynamic" require="true" visible="false">
        <description><![CDATA[The thickness of the border around the slider Focus]]></description>
        <value><![CDATA[{%= Theme.BorderWidth %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="showValue" name="Show value on handle" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Show value on handle]]></description>
        <value><![CDATA[1]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="handleText" name="Text to add on handle" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[Text to add on handle]]></description>
        <value></value>
      </property>
      <property xsi:type="standardProperty" id="handleTextPosition" name="Handle text position" type="string" mode="dynamic" require="false" visible="true">
        <description><![CDATA[The position of the text added on handle]]></description>
        <value><![CDATA[right]]></value>
        <options>
          <option value="left" text="Left" />
          <option value="right" text="Right" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="handleFontColour" name="Handle Font Colour" type="color" mode="dynamic" require="true" visible="false">
        <description><![CDATA[Handle font colour]]></description>
        <value><![CDATA[{%= Theme.BlackColor %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="useHandleImage" name="Use Handle Image" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Turn this on to use an image in the handle]]></description>
        <value><![CDATA[0]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="handleImagePath" name="Handle Image Path" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[Path and filename of image to be used for handle]]></description>
        <value></value>
      </property>
      <property xsi:type="standardProperty" id="handleImageWidth" name="Handle Image Width" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[Handle image width]]></description>
        <value></value>
      </property>
      <property xsi:type="standardProperty" id="handleImageHeight" name="Handle Image Height" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[Handle image height]]></description>
        <value></value>
      </property>
      <property xsi:type="standardProperty" id="hideHandleBG" name="Hide Handle Background" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Turn this on to hide the handle background and only display the image]]></description>
        <value><![CDATA[0]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
    </category>
    <category id="looped" name="Looped Questions">
      <property xsi:type="standardProperty" id="isInLoop" name="Is in loop" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Turn this on to use the control in a loop]]></description>
        <value><![CDATA[0]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="hideCaption" name="Hide Caption" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Turn this on to hide the caption]]></description>
        <value><![CDATA[0]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="captionAlign" name="Caption Alignment" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The alignment of the text in the caption when the question is looped]]></description>
        <value><![CDATA[center]]></value>
        <options>
          <option value="left" text="Left" />
          <option value="center" text="Center" />
          <option value="right" text="Right" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="captionPlacement" name="Caption Placement" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The placement of the caption when the question is looped]]></description>
        <value><![CDATA[top]]></value>
        <options>
          <option value="top" text="Top" />
          <option value="bottom" text="Bottom" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="captionWidth" name="Caption Width" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The width of the caption]]></description>
        <value><![CDATA[auto]]></value>
      </property>
      <property xsi:type="standardProperty" id="captionPadding" name="Caption Padding" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[Padding around caption text]]></description>
        <value><![CDATA[10px 0]]></value>
      </property>
      <property xsi:type="standardProperty" id="imageAlign" name="Image Alignment" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The alignment of the text in the responses]]></description>
        <value><![CDATA[right]]></value>
        <options>
          <option value="left" text="Left" />
          <option value="center" text="Center" />
          <option value="right" text="Right" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="interconnection" name="Interconnection between sliders" type="string" mode="dynamic" require="true" visible="true">
        <description><![CDATA[Connect each slider to the others within the same loop, so that the sum of their value to not go beyond a certain maximum.
      A checkbox is added to each slider, freezing them at their current position.
      Must be used on numeric question sliders, within a loop.]]></description>
        <value><![CDATA[0]]></value>
        <options>
          <option value="1" text="Yes" />
          <option value="0" text="No" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="freezeBoxLabel" name="Label for the freeze checkbox" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The label for the checkbox freezing the current slider]]></description>
        <value><![CDATA[Freeze this slider]]></value>
      </property>
    </category>
    <category id="general" name="General">
      <property xsi:type="standardProperty" id="fontSize" name="Font Size" type="string" mode="dynamic" require="true" visible="false">
        <description><![CDATA[The font size for the control]]></description>
        <value><![CDATA[{%= Theme.NormalFS %}]]></value>
      </property>
      <property xsi:type="standardProperty" id="controlWidth" name="Control Width" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The width of the control]]></description>
        <value><![CDATA[100%]]></value>
      </property>
      <property xsi:type="standardProperty" id="maxWidth" name="Max Width" type="string" mode="static" require="false" visible="true">
        <description><![CDATA[The maximum width of the control]]></description>
        <value><![CDATA[600px]]></value>
      </property>
      <property xsi:type="standardProperty" id="controlAlign" name="Control Alignment" type="string" mode="dynamic" require="false" visible="true">
        <description><![CDATA[The alignment of the control]]></description>
        <value><![CDATA[center]]></value>
        <options>
          <option value="left" text="Left" />
          <option value="center" text="Center" />
          <option value="right" text="Right" />
        </options>
      </property>
      <property xsi:type="standardProperty" id="textAlign" name="Text Alignment" type="string" mode="dynamic" require="false" visible="true">
        <description><![CDATA[The alignment of the text in the responses]]></description>
